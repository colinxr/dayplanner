{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "airbnb-base",
        "@nuxtjs"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": ["error", "never"],
        "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": ["test/**"],
              "optionalDependencies": false,
              "peerDependencies": false
            }
          ]
    }
}
