{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:@typescript-eslint/recommended",
      "airbnb-base"
  ],
  "overrides": [
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": ["./tsconfig.json"]
  },
  "plugins": [
      "vue"
  ],
  "rules": {
      "semi": ["error", "never"],
      "import/extensions": [
          "error",
          "ignorePackages",
          {
            "": "never",
            "js": "never",
            "jsx": "never",
            "ts": "never",
            "tsx": "never"
          }
       ]
  },
  "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      },
      
    }
}
